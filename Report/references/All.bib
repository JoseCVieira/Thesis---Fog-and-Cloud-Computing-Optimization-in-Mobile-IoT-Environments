	File = {:Users/diogoraposo/Library/Application Support/Mendeley Desktop/Downloaded/Khalili et al. - 2013 - MPTCP is not pareto-optimal Performance issues and a possible solution.pdf:pdf},
	Isbn = {9781450317757},
	Issn = {10636692},
	Journal = {IEEE/ACM Transactions on Networking},
	Keywords = {Congestion control algorithm,Multipath TCP,Performance evaluation,Protocol design},
	Number = {5},
	Pages = {1651--1665},
	Title = {{MPTCP is not pareto-optimal: Performance issues and a possible solution}},
