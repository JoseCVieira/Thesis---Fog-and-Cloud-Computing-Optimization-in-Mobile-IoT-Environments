@article{Stribling2009,
	Abstract = {WheelFS is a wide-area distributed storage system in- tended to help multi-site applications share data and gain fault tolerance. WheelFS takes the form of a distributed file system with a familiar POSIX interface. Its design al- lows applications to adjust the tradeoff between prompt visibility of updates from other sites and the ability for sites to operate independently despite failures and long delays. WheelFS allows these adjustments via semantic cues, which provide application control over consistency, failure handling, and file and replica placement. WheelFS is implemented as a user-level file system and is deployed on PlanetLab and Emulab. Three applications (a distributed Web cache, an email service and large file distribution) demonstrate that WheelFS's file system in- terface simplifies construction of distributed applications by allowing reuse of existing software. These applica- tions would perform poorly with the strict semantics im- plied by a traditional file system interface, but by pro- viding cues to WheelFS they are able to achieve good performance. Measurements show that applications built on WheelFS deliver comparable performance to services such as CoralCDN and BitTorrent that use specialized wide-area storage systems.},
	Author = {Stribling, Jeremy and Sovran, Yair and Zhang, Irene and Pretzer, Xavid and Li, Jinyang and Kaashoek, M Frans and Morris, Robert},
	Date-Added = {2017-01-26 17:21:30 +0000},
	Date-Modified = {2017-01-26 17:21:30 +0000},
	File = {:E$\backslash$:/Work/Faculdade/Mestrado/2015{\_}2016/1 sem/projecto tese/papers/mpc-secure cloud storage/4 distributed and cloud file systems/nsdi09-Flexible Wide-Area Storage for Distributed Systems with WheelFS.pdf:pdf;:E$\backslash$:/Work/Faculdade/Mestrado/2015{\_}2016/1 sem/projecto tese/papers/mpc-secure cloud storage/4 distributed and cloud file systems/[Anotado]nsdi09-Flexible Wide-Area Storage for Distributed Systems with WheelFS.pdf:pdf},
	Journal = {Proceedings of the 6th {\{}USENIX{\}} {\{}S{\}}ymposium on {\{}N{\}}etworked {\{}S{\}}ystems {\{}D{\}}esign and {\{}I{\}}mplementation ({\{}NSDI{\}} '09)},
