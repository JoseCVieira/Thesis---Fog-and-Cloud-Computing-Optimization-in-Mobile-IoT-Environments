	Isbn = {978-1-4673-5525-4},
	Journal = {Proceedings of the 2013 IEEE/ACM International Symposium on Code Generation and Optimization (CGO)},
	Keywords = {automated software diversity,cold-code,com-,nop insertion,pilers,profiling},
	Mendeley-Groups = {Thesis' Project Related Work/2. Implementing diversity for security},
	Pages = {1--11},
