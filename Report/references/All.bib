	Issn = {1545-5971},
	Journal = {IEEE Transactions on Dependable and Secure Computing},
	Number = {4},
	Pages = {273--288},
	Title = {{An experimental study of internet path diversity}},
	Url = {http://ieeexplore.ieee.org/xpls/abs{\_}all.jsp?arnumber=4012641},
