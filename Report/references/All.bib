@article{Larsen2014,
	Abstract = {Most software attacks rely on some predictable behavior on the target platform, and so mass distribution of identical software facilitates mass exploitation. As a countermeasure, there has been a resurgent interest in moving target defenses in general and biologically inspired artificial software diversity in particular. While the concept of software diversity has interested researchers for over twenty years, technical obstacles have prevented its widespread adoption until now. But now, massive-scale software diversity has become practical due to the Internet (enabling distribution of individualized software) and cloud computing (computational power to perform diversification). In this article, we take stock of the current state of software diversity research, and what we see is quite encouraging. It turns out that the potential show-stopper issues are now mostly solved, but a few lesser concerns still separate us from widespread adoption. We describe these remaining issues and point to a realistic adoption path.},
	Author = {Larsen, P. and Brunthaler, S. and Franz, M.},
	Date-Added = {2017-01-26 09:59:36 +0000},
